#include "Kortstokk.hpp"
#include <vector>
#include <iostream>

using namespace std;

Kortstokk::Kortstokk() : 
  stokken{Kort{'h',(int)1}, Kort{'h',(int)2}};
{
  Kort k = stokken.begin();
  cout << "Laget kort: " << k.suite() << k.value() << endl;
  k++;
  cout << "Laget kort: " << k.suite() << k.value() << endl;
}
